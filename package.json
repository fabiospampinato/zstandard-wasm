{
  "name": "zstandard-wasm",
  "description": "A fast and small port of Zstandard to WASM. (Decompress-only for now).",
  "version": "1.5.0",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "clean": "rimraf dist",
    "compile": "tsc --skipLibCheck && tstei",
    "compile:watch": "tsc --skipLibCheck --watch",
    "test": "node test/index.js",
    "update": "bash zstd/update.sh",
    "prepublishOnly": "npm run clean && npm run compile && npm run test"
  },
  "bugs": {
    "url": "https://github.com/fabiospampinato/zstandard-wasm/issues"
  },
  "license": "MIT",
  "author": {
    "name": "Fabio Spampinato",
    "email": "spampinabio@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/fabiospampinato/zstandard-wasm.git"
  },
  "keywords": [
    "zstd",
    "zstandard",
    "decompress",
    "wasm",
    "fast",
    "small"
  ],
  "dependencies": {
    "decode-base64": "^1.0.0"
  },
  "devDependencies": {
    "@types/node": "^15.12.1",
    "rimraf": "^3.0.2",
    "typescript": "^4.3.2",
    "typescript-transform-export-interop": "^1.0.4"
  }
}
